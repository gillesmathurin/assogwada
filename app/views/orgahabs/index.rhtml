<h1>Listing orgahabs</h1>

<table>
  <tr>
    <th>Nom</th>
  </tr>
  
<% for orgahab in @orgahabs %>
  <tr>
    <td><%=h orgahab.nom %></td>
    <td><%= link_to 'Show', orgahab_path(orgahab) %></td>
    <td><%= link_to 'Edit', edit_orgahab_path(orgahab) %></td>
    <td><%= link_to 'Destroy', orgahab_path(orgahab), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New orgahab', new_orgahab_path %>